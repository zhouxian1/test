<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.geovis.mapper.ResourcesMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.geovis.entity.Resources">
		<id column="id" property="id" />
		<result column="user_name" property="userName" />
		<result column="res_url" property="resUrl" />
		<result column="user_type" property="userType" />
		<result column="parent_id" property="parentId" />
		<result column="user_sort" property="userSort" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_name AS userName, res_url AS resUrl, user_type AS userType, parent_id AS parentId, user_sort AS userSort
    </sql>

</mapper>
